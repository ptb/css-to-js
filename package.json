{
  "activationEvents": [
    "onCommand:ptb.cssToJs"
  ],
  "author": {
    "email": "peter@ptb.dev",
    "name": "Peter T Bosse II",
    "url": "https://ptb.dev/"
  },
  "bugs": {
    "url": "https://github.com/ptb/css-to-js/issues"
  },
  "categories": [
    "Formatters"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ptb.cssToJs",
        "title": "Convert CSS to JS"
      }
    ]
  },
  "dependencies": {
    "@ptb/style": "latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "7.32.0",
    "eslint-plugin-compat": "latest",
    "eslint-plugin-fp": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-json": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-promise": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-standard": "latest",
    "prettier-eslint": "latest",
    "prettier-eslint-cli": "latest",
    "typescript": "latest"
  },
  "displayName": "Convert CSS to JS",
  "engines": {
    "vscode": ">=1.0.0"
  },
  "license": "Apache-2.0",
  "main": "./extension.js",
  "name": "css-to-js",
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "parser": "babel",
    "printWidth": 70,
    "proseWrap": "always",
    "requirePragma": false,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "publisher": "ptb",
  "repository": {
    "type": "git",
    "url": "https://github.com/ptb/css-to-js"
  },
  "scripts": {
    "lint": "eslint --fix src",
    "tidy": "prettier-eslint --write \"src/**/*.{js,jsx}\""
  },
  "type": "commonjs",
  "version": "1.0.1"
}
